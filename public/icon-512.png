<svg width="512" height="512" viewBox="0 0 512 512" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <linearGradient id="bg" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#0f172a;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#1e293b;stop-opacity:1" />
    </linearGradient>
    <linearGradient id="tree" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#22c55e;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#16a34a;stop-opacity:1" />
    </linearGradient>
  </defs>
  
  <!-- Background -->
  <rect width="512" height="512" fill="url(#bg)" rx="64"/>
  
  <!-- Tree trunk -->
  <rect x="236" y="320" width="40" height="120" fill="#92400e" rx="4"/>
  
  <!-- Tree leaves (main) -->
  <circle cx="256" cy="280" r="80" fill="url(#tree)"/>
  
  <!-- Tree leaves (left) -->
  <circle cx="200" cy="300" r="50" fill="url(#tree)" opacity="0.9"/>
  
  <!-- Tree leaves (right) -->
  <circle cx="312" cy="300" r="50" fill="url(#tree)" opacity="0.9"/>
  
  <!-- Tree leaves (top) -->
  <circle cx="256" cy="220" r="45" fill="url(#tree)" opacity="0.8"/>
  
  <!-- Seedling at base -->
  <g transform="translate(180, 380)">
    <rect x="8" y="20" width="4" height="15" fill="#16a34a"/>
    <ellipse cx="10" cy="18" rx="8" ry="6" fill="#22c55e"/>
  </g>
  
  <!-- Another seedling -->
  <g transform="translate(320, 390)">
    <rect x="6" y="15" width="3" height="12" fill="#16a34a"/>
    <ellipse cx="7.5" cy="13" rx="6" ry="4" fill="#22c55e"/>
  </g>
  
  <!-- Sparkles/growth effect -->
  <g fill="#fbbf24" opacity="0.8">
    <circle cx="180" cy="240" r="3"/>
    <circle cx="340" cy="260" r="2"/>
    <circle cx="160" cy="320" r="2"/>
    <circle cx="360" cy="340" r="3"/>
  </g>
</svg>