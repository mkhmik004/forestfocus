<svg width="1200" height="630" viewBox="0 0 1200 630" fill="none" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <linearGradient id="bg" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" stop-color="#0f172a"/>
      <stop offset="100%" stop-color="#1e293b"/>
    </linearGradient>
  </defs>
  
  <rect width="1200" height="630" fill="url(#bg)"/>
  
  <!-- Background forest silhouette -->
  <g opacity="0.1">
    <path d="M0 500 Q100 450 200 480 T400 460 T600 470 T800 450 T1000 460 T1200 440 V630 H0 Z" fill="#10b981"/>
  </g>
  
  <!-- Main content -->
  <g transform="translate(600, 315)">
    <!-- Title -->
    <text x="0" y="-150" text-anchor="middle" fill="#10b981" font-family="Arial, sans-serif" font-size="72" font-weight="bold">ForestFocus</text>
    <text x="0" y="-100" text-anchor="middle" fill="#64748b" font-family="Arial, sans-serif" font-size="28">Grow your productivity forest on Base</text>
    
    <!-- Central tree -->
    <g transform="translate(0, -20)">
      <path d="M0 -80C0 -80 40 -100 40 -60C40 -20 0 0 0 0C0 0 -40 -20 -40 -60C-40 -100 0 -80 0 -80Z" fill="#22c55e"/>
      <rect x="-6" y="0" width="12" height="60" fill="#8b5cf6"/>
      <ellipse cx="0" cy="70" rx="30" ry="10" fill="#16a34a" opacity="0.6"/>
    </g>
    
    <!-- Side trees -->
    <g transform="translate(-150, 20)">
      <path d="M0 -50C0 -50 25 -65 25 -35C25 -5 0 10 0 10C0 10 -25 -5 -25 -35C-25 -65 0 -50 0 -50Z" fill="#16a34a"/>
      <rect x="-4" y="10" width="8" height="40" fill="#8b5cf6"/>
    </g>
    
    <g transform="translate(150, 10)">
      <path d="M0 -60C0 -60 30 -75 30 -45C30 -15 0 0 0 0C0 0 -30 -15 -30 -45C-30 -75 0 -60 0 -60Z" fill="#15803d"/>
      <rect x="-5" y="0" width="10" height="50" fill="#8b5cf6"/>
    </g>
    
    <!-- Pomodoro timer -->
    <g transform="translate(-250, -50)">
      <circle cx="0" cy="0" r="40" fill="#ef4444" stroke="#ffffff" stroke-width="3"/>
      <text x="0" y="8" text-anchor="middle" fill="#ffffff" font-family="Arial, sans-serif" font-size="18" font-weight="bold">25:00</text>
    </g>
    
    <!-- LeafCoins -->
    <g transform="translate(250, -30)">
      <circle cx="0" cy="0" r="20" fill="#fbbf24"/>
      <text x="0" y="6" text-anchor="middle" fill="#ffffff" font-family="Arial, sans-serif" font-size="16" font-weight="bold">LC</text>
      <text x="0" y="40" text-anchor="middle" fill="#fbbf24" font-family="Arial, sans-serif" font-size="14">LeafCoin</text>
    </g>
    
    <!-- Features -->
    <text x="-200" y="120" fill="#64748b" font-family="Arial, sans-serif" font-size="16">🍅 Focus Sessions</text>
    <text x="0" y="120" text-anchor="middle" fill="#64748b" font-family="Arial, sans-serif" font-size="16">🌱 Grow Trees</text>
    <text x="200" y="120" text-anchor="end" fill="#64748b" font-family="Arial, sans-serif" font-size="16">💰 Earn Rewards</text>
    
    <!-- Call to action -->
    <rect x="-100" y="140" width="200" height="50" rx="25" fill="#10b981"/>
    <text x="0" y="172" text-anchor="middle" fill="#ffffff" font-family="Arial, sans-serif" font-size="18" font-weight="bold">Start Growing</text>
  </g>
  
  <!-- Decorative elements -->
  <circle cx="100" cy="100" r="4" fill="#fbbf24" opacity="0.6"/>
  <circle cx="1100" cy="150" r="3" fill="#fbbf24" opacity="0.8"/>
  <circle cx="200" cy="200" r="2" fill="#fbbf24" opacity="0.7"/>
  <circle cx="1000" cy="250" r="3" fill="#fbbf24" opacity="0.5"/>
  
  <!-- Base logo/text -->
  <text x="600" y="580" text-anchor="middle" fill="#64748b" font-family="Arial, sans-serif" font-size="14">Built on Base • Powered by OnchainKit</text>
</svg>